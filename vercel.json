{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/logo.ico",
      "status": 204
    },
    {
      "src": "/(.*)",         
      "dest": "/index.html"   
    }
  ]
}