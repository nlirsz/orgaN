{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.@(html|css|js|png|ico)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "/src/home/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
