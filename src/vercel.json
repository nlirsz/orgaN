{
  "version": 2,
  "public": "public",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/((?!api/|favicon.ico|logo.ico|style.css|renderer.js|logo.png|.*\\.).*)", "destination": "/public/index.html" }
  ]
}